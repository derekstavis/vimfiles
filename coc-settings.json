{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "javascriptreact",
    "javascript",
    "javascript.jsx"
  ],
  "suggest.snippetIndicator": " ⭐︎",

  "inlayHint.enable": true,
  "inlayHint.display": true,
  "inlayHint.enableParameter": true,
  "inlayHint.refreshOnInsertMode": true,

  "cSpell.language": "en",

  "coc.preferences.previewAutoClose": false,
  "coc.preferences.hoverTarget": "float",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.formatOnType": true,
  "hover.previewMaxHeight": 30,

  "importCost.typescriptExtensions": ["\\.tsx?$"],
  "importCost.javascriptExtensions": ["\\.jsx?$"],
  "importCost.bundleSizeDecoration": "both",
  "importCost.showCalculatingDecoration": true,
  "importCost.debug": false,

  "codeLens.enable": true,

  "list.floatPreview": true,
  "suggest.floatConfig.winblend": 20,
  "diagnostic.floatConfig.winblend": 20,
  "signature.floatConfig.winblend": 20,
  "hover.floatConfig.winblend": 20,
  "floatFactory.floatConfig.winblend": 20,
  "outline.autoPreview": true,

  "prettier.tslintIntegration": true,
  "tsserver.useLocalTsdk": true,
  "tsserver.maxTsServerMemory": 16000,

  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,

  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterNames.suppressWhenArgumentMatchesName": true,

  "typescript.preferences.quoteStyle": "double",
  "semanticTokens.enable": true,
  "biome.trace.server": "verbose",

  "explorer.keyMappings.global": {
    "i": false,
    "<c-o>": "noop",

    "*": "toggleSelection",
    "m": ["wait", "actionMenu"],

    "gk": ["wait", "expandablePrev"],
    "gj": ["wait", "expandableNext"],
    "h": ["wait", "collapse"],
    "l": ["wait", "expandable?", "expand", "open"],
    "J": ["wait", "toggleSelection", "normal:j"],
    "K": ["wait", "toggleSelection", "normal:k"],
    "gl": ["wait", "expand:recursive"],
    "gh": ["wait", "collapse:recursive"],
    "o": ["wait", "open"],
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "e": "open",
    "es": "open:split",
    "ev": "open:vsplit",
    "et": "open:tab",
    "<bs>": ["wait", "gotoParent"],
    "gs": ["wait", "reveal:select"],
    "il": "preview:labeling",
    "ic": "preview:content",
    "Il": "previewOnHover:toggle:labeling",
    "Ic": "previewOnHover:toggle:content",
    "II": "previewOnHover:disable",

    "yp": "copyFilepath",
    "yn": "copyFilename",
    "yy": "copyFile",
    "dd": "cutFile",
    "p": "pasteFile",
    "df": "delete",
    "dF": "deleteForever",

    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",

    "zh": "toggleHidden",
    "g<dot>": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "<esc>": "esc",
    "X": "systemExecute",
    "gd": "listDrive",

    "f": "search",
    "F": "searchRecursive",

    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",

    "[[": ["wait", "sourcePrev"],
    "]]": ["wait", "sourceNext"],

    "[i": ["wait", "indentPrev"],
    "]i": ["wait", "indentNext"],

    "[m": ["wait", "markPrev:modified"],
    "]m": ["wait", "markNext:modified"],

    "[d": ["wait", "markPrev:diagnosticError:diagnosticWarning"],
    "]d": ["wait", "markNext:diagnosticError:diagnosticWarning"],
    "[D": ["wait", "markPrev:diagnosticError"],
    "]D": ["wait", "markNext:diagnosticError"],

    "[c": ["wait", "markPrev:git"],
    "]c": ["wait", "markNext:git"],
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "cSpell.userWords": ["rssi", "wifi"]
}
